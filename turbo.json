{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ]
        },
        "test": {
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "persistent": true,
            "dependsOn": [
                "@appwrite.io/pink-icons#build"
            ]
        },
        "lint": {
            "cache": false
        },
        "format": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}