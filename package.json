{
  "name": "@appwrite/monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --no-cache",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "turbo run format",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "turbo": "^1.2.8",
    "vite": "^2.9.8"
  },
  "engines": {
    "npm": ">=9.1.3",
    "node": ">=18.12.1"
  },
  "packageManager": "npm@9.1.3"
}
